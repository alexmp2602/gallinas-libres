---
import Layout from "../layouts/Layout.astro";
import Hero from "../components/Hero.astro";
import Mision from "../components/Mision.astro";
import Beneficios from "../components/Beneficios.astro";
import Produccion from "../components/Produccion.astro";
import Contacto from "../components/Contacto.astro";
import Ubicacion from "../components/Ubicacion.astro";
---

<Layout title="Gallinas Libres">
  <Fragment slot="head">
    <link
      rel="preload"
      as="image"
      href="/img/hero/hero-gallinas-995.avif"
      imagesrcset="/img/hero/hero-gallinas-768.avif 768w, /img/hero/hero-gallinas-995.avif 995w"
      imagesizes="100vw"
      fetchpriority="high"
    />
  </Fragment>

  <Hero />
  <Mision />
  <Beneficios />
  <Produccion />
  <Contacto />
  <Ubicacion />
</Layout>
